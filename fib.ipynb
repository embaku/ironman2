   ],
   "source": [
    "import math\n",
    "# Golden ratio\n",
    "gr = (1+math.sqrt(5))/2\n",
    "\n",
    "def fib(n):\n",
    "    for i in range(n):\n",
    "        result = round(pow(gr, i)/math.sqrt(5))\n",
    "        print(result, end=\" \")\n",
    "\n",
    "n = int(input(\"Enter length of fibonacci series: \"))\n",
    "fib(n)"
   ]
  }
